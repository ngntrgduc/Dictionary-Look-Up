{
    "name": "Dictionary Look Up",
    "version": "2.12.0",
    "description": "Extension to look up words painlessly",
    "manifest_version": 3,
    "author": "Nguyen Trung Duc",
    "icons": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "128": "assets/icon128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "Dictionary Look Up",
        "default_icon": "assets/icon.png"
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+O"
            }
        }
    }
}
